{
  "name": "corde",
  "version": "2.0.0-beta.4",
  "description": "A simple library for Discord bot tests",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "directories": {
    "lib": "bin"
  },
  "bin": "bin/corde",
  "keywords": [
    "discord-bot",
    "discord-js",
    "nodejs",
    "testing",
    "discord",
    "corde",
    "test",
    "cli",
    "typescript-library",
    "typescript",
    "automated-testing",
    "bot"
  ],
  "scripts": {
    "build": "bash tasks.sh build",
    "watch": "bash tasks.sh watch",
    "corde": "node ./bin/corde",
    "test": "jest --silent --runInBand --detectOpenHandles",
    "test-with-coverage": "jest --silent --runInBand --detectOpenHandles --collect-coverage --collectCoverageFrom=src/**/*.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && yarn install",
    "fix-lint": "tslint --fix 'src/**/*.ts' 'tests/**/*.ts",
    "np": "np --no-cleanup --no-yarn",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --project tsconfig.json -t stylish",
    "validate-ci": "bash tasks.sh validade-ci",
    "jestClear": "jest --clearCache",
    "init-test-bot": "bash tasks.sh init-test-bot",
    "build-bot": "bash tasks.sh build-test-bot",
    "task": "bash tasks.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@microsoft/tsdoc": "^0.12.20",
    "@types/esm": "^3.2.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.22",
    "@types/require-from-string": "^1.2.0",
    "@types/rewire": "^2.5.28",
    "@types/ws": "^7.2.4",
    "babel-jest": "^26.0.1",
    "cli-spinners": "^2.4.0",
    "codecov": "^3.6.5",
    "dotenv": "^8.2.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "jest-mock-process": "^1.4.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^3.0.0",
    "require-from-string": "^2.0.2",
    "rewire": "^5.0.0",
    "semver": "^7.3.2",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.3.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.0.0",
    "commander": "^6.0.0",
    "discord.js": "^12.2.0",
    "esm": "^3.2.25",
    "figlet": "^1.2.4",
    "ora": "^5.0.0",
    "path": "^0.12.7",
    "rxjs": "^6.5.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/corde.git"
  },
  "author": {
    "name": "Lucas Gomes",
    "email": "lucasgsm88@gmail.com",
    "url": "https://github.com/lucasgmagalhaes"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/corde/issues"
  },
  "homepage": "https://github.com/lucasgmagalhaes/corde#readme",
  "files": [
    "lib/**",
    "bin/"
  ]
}
